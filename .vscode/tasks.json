{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-std=c99",
                "-D_XOPEN_SOURCE=600",
                "-D_DEFAULT_SOURCE",
                "-pedantic",
                "-Wall",
                "-W",
                "-Wmissing-prototypes",
                "-Wno-sign-compare",
                "-Wimplicit-fallthrough",
                "-Wno-unused-parameter",
                "-I${workspaceFolder}/lib",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "${workspaceFolder}/libtlpi.a"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}